{
  "name": "serverest-cypress",
  "version": "1.0.0",
  "description": "Projeto de estudo de testes back-end com Cypress para o simulador de loja virtual ServeRest API",
  "main": "index.js",
  "scripts": {
    "lint": "eslint cypress/**/*.js",
    "lint:fix": "eslint cypress/**/*.js --fix",
    "test": "cypress run",
    "cy:open": "cypress open",
    "test:postLogin": "cypress run --spec cypress/integration/login/postLogin.spec.js",
    "test:deleteProducts": "cypress run --spec cypress/integration/products/deleteProducts.spec.js",
    "test:getProducts": "cypress run --spec cypress/integration/products/getProducts.spec.js",
    "test:postProducts": "cypress run --spec cypress/integration/products/postProducts.spec.js",
    "test:putProducts": "cypress run --spec cypress/integration/products/putProducts.spec.js",
    "test:deleteUsers": "cypress run --spec cypress/integration/users/deleteUsers.spec.js",
    "test:getUsers": "cypress run --spec cypress/integration/users/getUsers.spec.js",
    "test:postUsers": "cypress run --spec cypress/integration/users/postUsers.spec.js",
    "test:putUsers": "cypress run --spec cypress/integration/users/putUsers.spec.js"
  },
  "author": "Bruno Quintanilha <braquintanilha@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@bahmutov/cy-api": "^2.1.3",
    "cypress": "^8.2.0",
    "eslint": "^7.32.0",
    "eslint-plugin-cypress": "^2.11.3",
    "faker": "^5.5.3",
    "joi": "^17.4.2"
  }
}
